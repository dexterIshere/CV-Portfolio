{
  "name": "cvportfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "migrate:dev": "yarn dotenv -e .env.local prisma migrate dev",
    "db:push": "yarn dotenv -e .env.local prisma db push",
    "migrate:reset": "yarn dotenv -e .env.local prisma migrate reset",
    "db:seed": "yarn dotenv -e .env.local prisma db seed",
    "prisma:generate": "yarn dotenv -e .env.local prisma generate",
    "prisma:resolve": "yarn dotenv -e .env.local prisma migrate resolve --applied <migration_name>",
    "prisma:studio": "yarn dotenv -e .env.local prisma studio",
    "production:build": "yarn prisma generate && npx prisma migrate deploy && next build"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "@tanstack/react-query": "^4.20.2",
    "@trpc/client": "^10.9.0",
    "@trpc/next": "^10.9.0",
    "@trpc/react-query": "^10.9.0",
    "@trpc/server": "^10.9.0",
    "@types/aos": "^3.0.4",
    "aos": "^2.3.4",
    "dotenv-cli": "^7.0.0",
    "gsap": "^3.11.4",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropdown": "^1.11.0",
    "react-icons": "^4.7.1",
    "react-parallax-tilt": "^1.7.110",
    "sass": "^1.58.3",
    "superjson": "1.9.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.21.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-next": "13.1.6",
    "prisma": "^4.9.0",
    "typescript": "^4.9.4"
  },
  "ct3aMetadata": {
    "initVersion": "7.5.6"
  }
}